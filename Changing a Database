CREATE TABLE ao3 (
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    name TEXT,
    story_type TEXT,
    published_works INTEGER);

INSERT INTO ao3 (name, story_type, published_works) VALUES ("Paulie", "suspense", 5);
